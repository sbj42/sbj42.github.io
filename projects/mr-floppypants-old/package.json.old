{
  "name": "mr-floppypants",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "James Clark <jameswclark@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true
    },
    "rules": {
      "indent": [
        "warn",
        4
      ],
      "no-tabs": "warn",
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "no-redeclare": "off",
      "semi": "error",
      "no-trailing-spaces": "warn",
      "no-extra-parens": "warn",
      "class-methods-use-this": "warn",
      "default-case": "error"
    }
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch"
  }
}
